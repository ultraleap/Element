intrinsic constraint Any
intrinsic function add(a, b)
intrinsic struct Num

namespace A
{
    x = 5
    namespace A
    {
        x = 10
        namespace A
        {
            x = 15
            namespace A
            {
                x = 20
            }
        }
        
        # Local A -> x = 15
        a = A.x
        
        # Local A -> nested A -> x = 20
        b = A.A.x
        
        # Local A -> nested A -> error A not found
        # Does not fall back! Matching is greedy.
        c = A.A.A.x
    }
}

namespace X
{
    x = 5
    namespace Y
    {
        x = 10
        namespace Z
        {
            x = 15
        }
        
        a = X.x
        b = Y.x
        c = Z.x
    }
}

makeAnonymousBlock(_a, _b) = { a = _a, b = _b }